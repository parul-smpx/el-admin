    "vutils": ">= 0.6.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "element-ui": "^1.2.1",
    "eslint": "3.16.1",
    "eslint-config-smartprix": "3.1.6",
    "mocha": "3.2.0",
    "sm-webpack-config": "^0.16.0",
    "vue": "^2.2.1",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1",
    "vutils": "^0.11.0"
  },
  "babel": {
    "presets": [
      "stage-0"
    ],
    "sourceMaps": true
