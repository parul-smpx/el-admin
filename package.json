{
  "name": "el-admin",
  "version": "0.2.2",
  "description": "Admin components using element (https://github.com/ElemeFE/element) tookit",
  "main": "src/index.js",
  "repository": "smartprix/el-admin",
  "directories": {
    "test": "test"
  },
  "scripts": {
